{
    "key": "space b",
    "command": "bookmarks.toggle",
    "when": "editorTextFocus && vim.active && vim.mode != 'Insert' && vim.mode != 'CommandlineInProgress'"
},
{
    "key": "ctrl+b",
    "command": "bookmarks.listAllFromFiles",
    "when": "editorTextFocus && vim.active && vim.mode != 'Insert' && vim.mode != 'CommandlineInProgress'"
},