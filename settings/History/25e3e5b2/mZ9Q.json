{
    "key": "ctrl+j",
    "command": "workbench.action.navigateDown",
    "when": "!terminalFocus"
},
{
    "key": "space y",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
    "text": "ls\u000D"
    },
    "when": "editorTextFocus && vim.active && vim.mode != 'Insert'"
},
{
    "key": "space t c",
    "command": "workbench.action.terminal.runRecentCommand",
    "when": "!terminalFocus"
},